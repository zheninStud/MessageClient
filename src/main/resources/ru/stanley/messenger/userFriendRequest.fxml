<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ru.stanley.messenger.Controllers.UserFriendRequestController"
            prefHeight="400.0" prefWidth="600.0">

    <VBox spacing="10" alignment="CENTER" padding="20">
        <Label fx:id="requestLabel"/>
        <Button fx:id="acceptButton" text="Принять" />
        <Button fx:id="denyButton" text="Отклонить"/>
    </VBox>
</AnchorPane>
